layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "pool4"
  bottom: "label"
  top: "accuracy"
  accuracy_param: {
	  top_k : 1
  }
}
layer {
  name: "accuracy_supercategory"
  type: "Accuracy"
  bottom: "pool3"
  bottom: "label_1"
  top: "accuracy_supercategory"
}
