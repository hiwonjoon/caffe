layer {
  name: "loss1"
  type: "SoftmaxWithLoss"
  bottom: "pool4"
  bottom: "label_2"
  top: "loss1"
  loss_weight: 0.3
}
layer {
  name: "loss2_supercategory"
  type: "SoftmaxWithLoss"
  bottom: "pool3"
  bottom: "label_1"
  top: "loss2_supercategory"
  loss_weight: 0.7
}
