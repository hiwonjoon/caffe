net: "train_val.prototxt"

test_iter: 1000
test_interval: 2000

lr_policy: "step"
base_lr: 0.01 #learning rate decreased factor of 10
stepsize: 100000
gamma: 0.1
max_iter: 300000

solver_type:SGD
momentum: 0.9

regularization_type : "L2";
weight_decay: 0.0005

display: 100
snapshot: 5000
snapshot_prefix: "vgg16"
solver_mode: GPU

debug_info: false 
